{
  "name": "@accounter/shaam6111-generator",
  "version": "0.1.0",
  "type": "module",
  "description": "Fully typed application that generates, parses, and validates SHAAM 6111 tax reports.",
  "repository": {
    "type": "git",
    "url": "https://github.com/gilgardosh/accounter-toolkit.git",
    "repository": "packages/shaam6111-generator"
  },
  "homepage": "https://github.com/gilgardosh/accounter-toolkit/packages/shaam6111-generator#readme",
  "bugs": {
    "url": "https://github.com/gilgardosh/accounter-toolkit/issues"
  },
  "author": "Gil Gardosh <gilgardosh@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": "^18.0.0 || >= 20"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/typings/index.d.cts",
        "default": "./dist/cjs/index.js"
      },
      "import": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "default": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "typings": "dist/typings/index.d.ts",
  "keywords": [
    "shaam",
    "shaam6111",
    "accountancy",
    "accountant",
    "accounter"
  ],
  "scripts": {
    "build": "yarn lint && bob build",
    "dev": "node --experimental-json-modules --import ../../scripts/register-esm.js src/index.ts",
    "lint": "eslint './src/**/*.{js,ts,tsx}' --quiet --fix",
    "prepublish": "yarn build",
    "start": "node ./dist",
    "test": "vitest",
    "test:watch": "vitest --watch"
  },
  "dependencies": {
    "zod": "^3.24.4",
    "iconv-lite": "^0.6.x"
  },
  "devDependencies": {
    "vitest": "^0.34.0"
  },
  "publishConfig": {
    "directory": "dist",
    "access": "public"
  },
  "typescript": {
    "definition": "dist/typings/index.d.ts"
  }
}
