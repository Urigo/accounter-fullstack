{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "array",
  "minItems": 1,
  "items": {
    "type": "object",
    "additionalProperties": false,
    "properties": {
      "bankNumber": {
        "type": "integer",
        "enum": [12]
      },
      "extendedBankNumber": {
        "type": "integer",
        "enum": [912]
      },
      "branchNumber": {
        "type": "integer",
        "minimum": 100,
        "maximum": 999
      },
      "accountNumber": {
        "type": "integer",
        "minimum": 10000,
        "maximum": 999999
      },
      "partyPreferredIndication": {
        "type": "integer",
        "enum": [0]
      },
      "partyAccountInvolvementCode": {
        "type": "integer",
        "enum": [1, 201]
      },
      "accountDealDate": {
        "type": "integer",
        "enum": [0]
      },
      "accountUpdateDate": {
        "anyOf": [
          {
            "type": "integer",
            "minimum": 10000000,
            "maximum": 99999999
          },
          {
            "type": "integer",
            "enum": [0]
          }
        ]
      },
      "metegDoarNet": {
        "type": "integer",
        "enum": [0, 1]
      },
      "kodHarshaatPeilut": {
        "type": "integer",
        "enum": [1, 3]
      },
      "accountClosingReasonCode": {
        "type": "integer",
        "enum": [0, 1]
      },
      "productLabel": {
        "description": "Branch number with account number and two spaces in between",
        "type": "string"
      },
      "accountAgreementOpeningDate": {
        "type": "integer",
        "enum": [0]
      },
      "serviceAuthorizationDesc": {
        "type": "string",
        "enum": ["לא חתום", "פעולות ומידע"]
      },
      "branchTypeCode": {
        "type": "integer",
        "enum": [0, 2]
      },
      "mymailEntitlementSwitch": {
        "type": "integer",
        "enum": [0, 1]
      }
    },
    "required": [
      "bankNumber",
      "extendedBankNumber",
      "branchNumber",
      "accountNumber",
      "partyPreferredIndication",
      "partyAccountInvolvementCode",
      "accountDealDate",
      "accountUpdateDate",
      "metegDoarNet",
      "kodHarshaatPeilut",
      "accountClosingReasonCode",
      "productLabel",
      "accountAgreementOpeningDate",
      "serviceAuthorizationDesc",
      "branchTypeCode",
      "mymailEntitlementSwitch"
    ]
  }
}
