{
  "name": "@accounter/modern-poalim-scraper",
  "version": "0.9.12",
  "type": "module",
  "description": "",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc && concurrently \"tsc -w\" \"nodemon dist/index.js\"",
    "format": "prettier \"**/*.ts\" --write",
    "json2ts": "json2ts -i src/schemas -o src/__generated__",
    "postbuild": "cp -r src/__generated__ dist/__generated__",
    "prebuild": "yarn json2ts",
    "start": "node dist/index.js"
  },
  "dependencies": {
    "ajv": "8.17.1",
    "ajv-formats": "3.0.1",
    "inquirer": "10.2.2",
    "node-fetch": "3.3.2",
    "puppeteer": "23.4.0",
    "uuid": "10.0.0"
  },
  "devDependencies": {
    "@types/inquirer": "9.0.7",
    "@types/node-fetch": "3.0.3",
    "@types/uuid": "10.0.0",
    "json-schema-to-typescript": "15.0.2"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "lint-staged": {
    "*.--write": "prettier --write"
  }
}
