{
  "name": "@accounter/modern-poalim-scraper",
  "version": "0.9.14",
  "type": "module",
  "description": "",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "yarn build && concurrently \"tsc -w\" \"nodemon dist/index.js\"",
    "format": "prettier \"**/*.ts\" --write",
    "json2ts": "json2ts -i src/schemas -o src/__generated__",
    "postbuild": "cp -r src/__generated__ dist/__generated__",
    "prebuild": "yarn json2ts",
    "start": "node dist/index.js",
    "test:max": "yarn build && concurrently \"tsc -w\" \"nodemon dist/test/scrape-max.js\""
  },
  "dependencies": {
    "ajv": "8.17.1",
    "ajv-formats": "3.0.1",
    "date-fns": "4.1.0",
    "inquirer": "12.3.2",
    "node-fetch": "3.3.2",
    "puppeteer": "24.1.0",
    "uuid": "11.0.5"
  },
  "devDependencies": {
    "@types/inquirer": "9.0.7",
    "@types/node-fetch": "3.0.3",
    "@types/uuid": "10.0.0",
    "json-schema-to-typescript": "15.0.4"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "lint-staged": {
    "*.--write": "prettier --write"
  }
}
