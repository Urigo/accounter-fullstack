{
  "name": "server",
  "version": "1.0.0",
  "type": "module",
  "description": "",
  "license": "MIT",
  "engines": {
    "node": "19.8.1"
  },
  "main": "./src/index.mts",
  "scripts": {
    "build": "tsc && tsc-alias",
    "build2": "tsc --project tsconfig.json && tscpaths -p tsconfig.json -s ./src -o ./dist",
    "dev": "nodemon --exec \"yarn start | pino-pretty\" --ignore dist --watch 'src/**/*' -e ts,tsx,mts",
    "generate": "cd .. && yarn codegen && cd server && yarn sqlgen",
    "pgtyped": "pgtyped -c pgconfig.json",
    "presqlgen": "rimraf **/*.types.ts",
    "prestart": "yarn build",
    "sqlgen": "dotenv -e ../.env yarn pgtyped",
    "start": "node dist/index.js"
  },
  "dependencies": {
    "@accounter-toolkit/green-invoice-graphql": "0.4.0-alpha-20230717092739-ac9a64c",
    "@accounter-toolkit/pcn874-generator": "0.2.0-alpha-20230326113533-8039ee4",
    "@envelop/graphql-modules": "4.0.6",
    "@graphql-tools/mock": "8.7.20",
    "@graphql-tools/schema": "9.0.19",
    "@graphql-yoga/node": "3.9.1",
    "@pgtyped/cli": "2.2.1",
    "@pgtyped/runtime": "2.1.0",
    "cloudinary": "1.37.0",
    "dataloader": "2.2.2",
    "date-fns": "2.30.0",
    "dotenv": "16.0.3",
    "dotenv-cli": "7.2.1",
    "fastify": "4.17.0",
    "graphql": "16.7.1",
    "graphql-modules": "2.1.2",
    "graphql-yoga": "3.9.1",
    "pg": "8.11.0",
    "reflect-metadata": "0.1.13"
  },
  "devDependencies": {
    "@types/pg": "8.6.6",
    "nodemon": "2.0.22",
    "pino-pretty": "10.0.0",
    "tsc-alias": "1.8.6"
  },
  "resolutions": {
    "graphql-modules/**/graphql": "16.6.0"
  }
}
