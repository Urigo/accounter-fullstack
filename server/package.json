{
  "name": "server",
  "version": "1.0.0",
  "type": "module",
  "description": "",
  "license": "MIT",
  "engines": {
    "node": "20.5.0"
  },
  "main": "./src/index.mts",
  "scripts": {
    "build": "tsc && tsc-alias",
    "build2": "tsc --project tsconfig.json && tscpaths -p tsconfig.json -s ./src -o ./dist",
    "dev": "nodemon --exec \"yarn start | pino-pretty\" --ignore dist --watch 'src/**/*' -e ts,tsx,mts",
    "generate": "cd .. && yarn codegen && cd server && yarn sqlgen",
    "pgtyped": "pgtyped -c pgconfig.json",
    "presqlgen": "rimraf **/*.types.ts",
    "prestart": "yarn build",
    "sqlgen": "dotenv -e ../.env yarn pgtyped",
    "start": "node dist/index.js"
  },
  "dependencies": {
    "@accounter-toolkit/green-invoice-graphql": "0.5.0",
    "@accounter-toolkit/pcn874-generator": "0.2.0",
    "@envelop/graphql-modules": "5.0.0",
    "@graphql-tools/mock": "9.0.0",
    "@graphql-tools/schema": "10.0.0",
    "@graphql-yoga/node": "3.9.1",
    "@pgtyped/cli": "2.2.1",
    "@pgtyped/runtime": "2.1.0",
    "cloudinary": "1.40.0",
    "dataloader": "2.2.2",
    "date-fns": "2.30.0",
    "dotenv": "16.3.1",
    "dotenv-cli": "7.2.1",
    "fastify": "4.21.0",
    "graphql": "16.7.1",
    "graphql-modules": "2.2.0",
    "graphql-yoga": "4.0.3",
    "pg": "8.11.2",
    "reflect-metadata": "0.1.13"
  },
  "devDependencies": {
    "@types/pg": "8.10.2",
    "nodemon": "3.0.1",
    "pino-pretty": "10.2.0",
    "tsc-alias": "1.8.7"
  },
  "resolutions": {
    "graphql-modules/**/graphql": "16.6.0"
  }
}
