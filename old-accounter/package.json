{
  "name": "old-accounter",
  "version": "0.0.0",
  "type": "module",
  "license": "MIT",
  "engines": {
    "node": "18.13.0"
  },
  "scripts": {
    "build": "tsc",
    "codegen": "graphql-codegen --config codegen.yml",
    "deploy": "git push heroku master",
    "dev": "tsc && concurrently \"tsc -w\" \"nodemon --experimental-specifier-resolution=node --experimental-json-modules dist/old-accounter/src/index.js\"",
    "generate": "concurrently \"yarn codegen\" \"cd server && yarn sqlgen\"",
    "get": "node --experimental-specifier-resolution=node dist/old-accounter/src/scrape.js",
    "lint": "eslint --ignore-path .gitignore .",
    "month": "node --experimental-specifier-resolution=node dist/taxMonthlyReport/taxesForMonth.js",
    "prettier": "prettier --ignore-path .gitignore --write --list-different .",
    "prettier:check": "prettier --ignore-path .gitignore --check .",
    "react:dev": "cd client && yarn dev",
    "react:install": "cd server && yarn install && cd ../temp-server && yarn install && cd ../client && yarn install && cd ../",
    "scrape": "node --experimental-specifier-resolution=node --experimental-json-modules dist/old-accounter/src/scrape.js",
    "server:dev": "cd server && yarn dev",
    "sqlgen": "npx pgtyped -c pgTypedConfig.json",
    "start": "node --experimental-specifier-resolution=node dist/old-accounter/src/index.js",
    "temp-server:dev": "cd temp-server && yarn dev"
  },
  "dependencies": {
    "date-fns": "2.29.3",
    "dotenv": "16.0.3",
    "graphql": "16.6.0",
    "lodash": "4.17.21",
    "modern-poalim-scraper": "0.6.7",
    "moment": "2.29.4",
    "pg": "8.8.0",
    "pixl-xml": "1.0.13",
    "uuid": "9.0.0"
  },
  "devDependencies": {
    "@types/lodash": "4.14.191",
    "@types/pg": "8.6.5",
    "@types/pixl-xml": "1.0.2",
    "@types/uuid": "9.0.0",
    "eslint": "8.29.0",
    "nodemon": "2.0.20"
  }
}
