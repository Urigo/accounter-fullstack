{
  "name": "modern-poalim-scraper",
  "version": "0.0.1",
  "description": "",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "node --experimental-specifier-resolution=node --experimental-json-modules dist/index.js",
    "dev": "tsc && concurrently \"tsc -w\" \"nodemon --experimental-specifier-resolution=node --experimental-json-modules dist/index.js\"",
    "build": "tsc",
    "format": "prettier \"**/*.ts\" --write",
    "json2ts": "json2ts -i src/schemas -o generatedTypes"
  },
  "dependencies": {
    "ajv": "6.12.5",
    "dotenv": "8.2.0",
    "inquirer": "7.3.3",
    "moment": "2.28.0",
    "node-fetch": "2.6.1",
    "puppeteer": "5.3.0",
    "uuid": "8.3.0"
  },
  "devDependencies": {
    "@types/dotenv": "8.2.0",
    "@types/inquirer": "7.3.1",
    "@types/node": "14.10.1",
    "@types/node-fetch": "2.5.7",
    "@types/puppeteer": "3.0.2",
    "@types/uuid": "8.3.0",
    "concurrently": "5.3.0",
    "json-schema-to-typescript": "9.1.1",
    "nodemon": "2.0.4",
    "prettier": "2.1.1",
    "typescript": "4.0.2"
  },
  "engines": {
    "node": "14.10.1"
  },
  "license": "MIT"
}
