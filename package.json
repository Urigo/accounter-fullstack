{
  "name": "modern-poalim-scraper",
  "version": "0.7.1",
  "description": "",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "start": "node --experimental-specifier-resolution=node --experimental-json-modules dist/index.js",
    "dev": "tsc && concurrently \"tsc -w\" \"nodemon --experimental-specifier-resolution=node --experimental-json-modules dist/index.js\"",
    "build": "json2ts -i src/schemas -o dist/generatedTypes && tsc",
    "format": "prettier \"**/*.ts\" --write",
    "json2ts": "json2ts -i src/schemas -o src/generatedTypes",
    "pre-commit": "lint-staged"
  },
  "dependencies": {
    "ajv": "8.12.0",
    "ajv-formats": "2.1.1",
    "inquirer": "9.1.4",
    "node-fetch": "3.3.0",
    "puppeteer": "19.7.2",
    "uuid": "9.0.0"
  },
  "devDependencies": {
    "@types/inquirer": "9.0.3",
    "@types/node": "18.14.4",
    "@types/node-fetch": "3.0.3",
    "@types/uuid": "9.0.1",
    "concurrently": "7.6.0",
    "husky": "8.0.3",
    "json-schema-to-typescript": "12.0.0",
    "lint-staged": "13.1.2",
    "nodemon": "2.0.21",
    "prettier": "2.8.4",
    "typescript": "4.9.5"
  },
  "engines": {
    "node": "19.6.1"
  },
  "license": "MIT",
  "lint-staged": {
    "*.--write": "prettier --write"
  }
}
