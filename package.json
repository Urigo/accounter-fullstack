{
  "name": "modern-poalim-scraper",
  "version": "0.0.1",
  "description": "",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "node --experimental-specifier-resolution=node --experimental-json-modules dist/index.js",
    "dev": "tsc && concurrently \"tsc -w\" \"nodemon --experimental-specifier-resolution=node --experimental-json-modules dist/index.js\"",
    "build": "tsc",
    "format": "prettier \"**/*.ts\" --write",
    "json2ts": "json2ts -i src/schemas -o generatedTypes",
    "pre-commit": "lint-staged"
  },
  "dependencies": {
    "ajv": "8.6.1",
    "ajv-formats": "2.1.0",
    "inquirer": "8.1.2",
    "node-fetch": "2.6.1",
    "puppeteer": "10.1.0",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@types/inquirer": "7.3.3",
    "@types/node": "15.3.0",
    "@types/node-fetch": "2.5.11",
    "@types/uuid": "8.3.1",
    "concurrently": "6.2.0",
    "husky": "7.0.1",
    "json-schema-to-typescript": "10.1.4",
    "lint-staged": "11.0.0",
    "nodemon": "2.0.10",
    "prettier": "2.3.2",
    "typescript": "4.3.5"
  },
  "engines": {
    "node": "16.3.0"
  },
  "license": "MIT",
  "lint-staged": {
    "*.--write": "prettier --write"
  }
}
