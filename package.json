{
  "type": "module",
  "scripts": {
    "start": "node --experimental-specifier-resolution=node dist/index.js",
    "dev": "tsc && concurrently \"tsc -w\" \"nodemon --experimental-specifier-resolution=node --experimental-json-modules dist/index.js\"",
    "scrape": "tsc && concurrently \"tsc -w\" \"nodemon --experimental-specifier-resolution=node --experimental-json-modules dist/scrape.js\"",
    "get": "node --experimental-specifier-resolution=node dist/scrape.js",
    "build": "tsc",
    "lint": "eslint --ignore-path .gitignore .",
    "prettier": "prettier --ignore-path .gitignore --write --list-different .",
    "prettier:check": "prettier --ignore-path .gitignore --check .",
    "deploy": "git push heroku master",
    "month": "node --experimental-specifier-resolution=node dist/taxMonthlyReport/taxesForMonth.js",
    "sqlgen": "npx pgtyped -c pgTypedConfig.json",
    "react:dev": "cd client && yarn dev",
    "react:install": "cd server && yarn install && cd ../temp-server && yarn install && cd ../client && yarn install && cd ../",
    "temp-server:dev": "cd temp-server && yarn dev",
    "server:dev": "cd server && yarn dev",
    "codegen": "graphql-codegen --config codegen.yml",
    "generate": "concurrently \"yarn codegen\" \"cd server && yarn sqlgen\""
  },
  "dependencies": {
    "date-fns": "2.28.0",
    "dotenv": "16.0.1",
    "graphql": "16.5.0",
    "lodash": "4.17.21",
    "modern-poalim-scraper": "0.3.2",
    "moment": "2.29.3",
    "pg": "8.7.3",
    "pixl-xml": "1.0.13",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.6.2",
    "@graphql-codegen/introspection": "2.1.1",
    "@graphql-codegen/typescript": "2.4.11",
    "@graphql-codegen/typescript-operations": "2.4.0",
    "@graphql-codegen/typescript-react-query": "3.5.12",
    "@graphql-codegen/typescript-resolvers": "2.6.4",
    "@theguild/eslint-config": "0.0.1",
    "@theguild/prettier-config": "0.0.1",
    "@types/dotenv": "8.2.0",
    "@types/lodash": "4.14.182",
    "@types/node": "17.0.32",
    "@types/pg": "8.6.5",
    "@types/pixl-xml": "1.0.2",
    "@types/uuid": "8.3.4",
    "concurrently": "7.1.0",
    "eslint": "8.15.0",
    "nodemon": "2.0.16",
    "prettier": "2.6.2",
    "typescript": "4.6.4"
  },
  "engines": {
    "node": "18.1.0"
  },
  "license": "MIT"
}
