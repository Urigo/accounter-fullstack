{
  "type": "module",
  "scripts": {
    "start": "node --experimental-specifier-resolution=node dist/index.js",
    "dev": "tsc && concurrently \"tsc -w\" \"nodemon --experimental-specifier-resolution=node --experimental-json-modules dist/index.js\"",
    "scrape": "tsc && concurrently \"tsc -w\" \"nodemon --experimental-specifier-resolution=node --experimental-json-modules dist/scrape.js\"",
    "get": "node --experimental-specifier-resolution=node dist/scrape.js",
    "build": "tsc",
    "format": "prettier \"**/*.ts\" --write",
    "deploy": "git push heroku master",
    "month": "node --experimental-specifier-resolution=node dist/taxMonthlyReport/taxesForMonth.js",
    "sqlgen": "npx pgtyped -c pgTypedConfig.json",
    "react:dev": "cd client && yarn dev",
    "server:dev": "cd temp-server && yarn dev"
  },
  "dependencies": {
    "date-fns": "2.28.0",
    "dotenv": "16.0.0",
    "lodash": "4.17.21",
    "modern-poalim-scraper": "0.2.9",
    "moment": "2.29.3",
    "pg": "8.7.3",
    "pixl-xml": "1.0.13",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@types/dotenv": "8.2.0",
    "@types/lodash": "4.14.182",
    "@types/node": "17.0.25",
    "@types/pg": "8.6.5",
    "@types/pixl-xml": "1.0.2",
    "@types/uuid": "8.3.4",
    "concurrently": "7.1.0",
    "nodemon": "2.0.15",
    "prettier": "2.6.2",
    "typescript": "4.6.3"
  },
  "engines": {
    "node": "18.0.0"
  },
  "license": "MIT"
}
